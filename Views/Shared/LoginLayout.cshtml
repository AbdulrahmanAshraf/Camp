    <link href="~/css/NoData.css" rel="stylesheet" type="text/css" />
    @*<link rel="stylesheet" href="/css/toastr.css">*@
    <!-- end inject -->
    @* for highlight map image *@
    <script src="~/mapper/cvi_tip_lib.js"></script>
    <script src="~/mapper/maputil.js"></script>
    <script src="~/mapper/mapper.js"></script>
    <link href="~/mapper/tooltip.css" rel="stylesheet" />

</head>
<body style="background-image:src(../../wwwroot/images/NVG_NEW_LOGO.svg)">

    <!-- start cssload-loader -->
    <div class="preloader">
        <div class="loader">
            <svg class="spinner" viewBox="0 0 50 50">
                <circle class="path" cx="25" cy="25" r="20" fill="none" stroke-width="5"></circle>
            </svg>
        </div>
    </div>

    @RenderBody()

    @*<div id="" class="back-to-btn"><a href="https://api.whatsapp.com/send?phone=+966507076049" target="_blank"><i class='lab la-whatsapp'></i></a></div>*@

    <!-- template js files -->
    <script src="/js/jquery-3.4.1.min.js"></script>
    <script src="/js/bootstrap.bundle.min.js"></script>
    <script src="/js/bootstrap-select.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/fancybox.js"></script>
    <script src="/js/jquery.lazy.min.js"></script>
    <script src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/jquery.counterup.min.js"></script>
    <script src="/js/main-rtl.js"></script>
    <script type="text/javascript" charset="utf8" src="https:////cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="@Url.Content("~/js/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/js/jquery.validate.unobtrusive.min.js")"></script>
    @*<script src="~/js/toastr.js"></script>*@


    @RenderSection("Scripts", required: false)


</body>
</html>
