    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Favicon -->
    <link rel="icon" sizes="16x16" href="images/vicon.png">

    <!-- inject:css -->
    <link rel="stylesheet" href="/css/bootstrap-rtl.min.css">
